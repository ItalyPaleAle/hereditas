  },
  "bugs": "https://github.com/ItalyPaleAle/hereditas/issues",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "2.2.0",
    "@oclif/command": "1.6.1",
    "@oclif/config": "1.15.1",
    "@oclif/plugin-help": "3.0.1",
